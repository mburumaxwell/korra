# EditorConfig is awesome: https://EditorConfig.org
root = false

[*]
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.c]
indent_style = space
indent_size = 4

[*.h]
indent_style = space
indent_size = 4

[CMakeLists.txt]
indent_style = space
indent_size = 2
