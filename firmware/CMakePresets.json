{
  "version": 2,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 20
  },
  "configurePresets": [
    {
      "name": "build-keeper-esp32s3_devkitc",
      "displayName": "Keeper - esp32s3_devkitc",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build-keeper-esp32s3_devkitc",
      "cacheVariables": {
        "NCS_TOOLCHAIN_VERSION": "NONE",
        "BOARD": "esp32s3_devkitc/esp32s3/procpu",
        "EXTRA_CONF_FILE": "apps/keeper.conf;local.conf",
        "EXTRA_DTC_OVERLAY_FILE": "apps/keeper.overlay"
      }
    },
    {
      "name": "build-keeper-frdm_rw612",
      "displayName": "Keeper - frdm_rw612",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build-keeper-frdm_rw612",
